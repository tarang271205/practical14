// Practical 14(3): Print Pattern. 
 
#include <stdio.h> 
void main() 
{ 
    int n,r,x; 
    printf("How Many Rows You Want??: "); 
    scanf("%d",&n); 
    printf("\n"); 
    // Here First Loop Stands For Rotates The Rows Of Patterns. 
    for(r=1 ; r<=n ; r++) 
    { 
        for(int c=1 ; c<=(n-r) ; c++) 
        { 
          printf("   "); 
        } 
         x=65; 
          // Here Second Loop Stands For Print The Elements Of Pattern Column Vise. 
        for(int c=1 ; c<=r ; c++) 
        { 
            printf(" %c " , x); 
            x++; 
        } 
        x = x-2; 
        for(int c=1 ; c<=r-1 ; c++) 
        { 
            printf(" %c " , x); 
            x--; 
        } 
        printf("\n"); 
    } 
    printf("\n\n\n24ce032_TARAN ");
}
 
