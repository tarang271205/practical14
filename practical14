    //Practical 14(2): Print Pattern 
 
 
 
#include <stdio.h> 
 
int main() { 
    int i, j, k; 
    int rows = 5; // Here Row Is Fixed. 
    //Here Loop Stands For Rows. 
    for (i = 1; i <= rows; i++) { 
        // Here First Inner Loop Stands For Put Space. 
        for (j = rows; j > i; j--) { 
            printf("   "); 
        } 
             printf("%d  ", k); 
        } 
 
        //Here Printf Stands For New Line. 
        printf("\n"); 
    } 
 
 
 
 
} 
