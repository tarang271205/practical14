  //Practical 14.1: Print Pattern. 
 
 
#include<stdio.h> 
void main() 
{   int x,y; 
   // Here Row Is Fixed. 
    int rows = 5; 
    //Here First Loop Stands For Each Row. 
    for (int x = 1; x <= rows; x++) { 
        //Here Inner Loop For Printing Values In Each Row. 
        for (int y = 1; y <= x; y++) { 
             // Here Condition For Check Row Is Even Or Odd. 
            if (y % 2 == 1) { 
               //Printing Number Like Print 1 For Even Row And 0 For Odd Rows. 
                printf("1 "); 
            } else { 
                printf("0 "); 
            } 
        } 
        // Here Printf Stand For New Line In Pattern.
            printf("\n"); 
    } 
 
 
} 
 
